{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "mongodb": "mongodb://localhost:27017/pc_back",
  "authentication": {
    //"secret": "",
    "secret": "supersecret",
    "strategies": [
      "jwt"
    ],
    "cookie": {
      "enabled": true, // whether the cookie should be enabled 
      "name": "feathers-jwt", // the cookie name 
      "httpOnly": false, // whether the cookie should not be available to client side JavaScript 
      "secure": true // whether cookies should only be available over HTTPS 
    },
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://unknown.com",
      "subject": "anonymous",
      "issuer": "feathers-123",
      "algorithm": "HS256",
      "expiresIn": "1d"
    }
  }
}
